INSERT INTO S_SYNC
	VALUES ("4ebd5f17-8c22-4f06-861d-517f5e19be2e",
	"00000000-0000-0000-0000-000000000000",
	'join',
	'',
	'select any user from instances of user where ( selected.endpoint == param.user );
if ( empty user )
  select any user from instances of user where ( selected.name == param.name );
  if ( empty user )
    create object instance user of user;
    user.name = param.name;
    user.endpoint = param.user;
    MSG::status( status:status::logged_in, message:"" );
  else
    MSG::status( status:status::error, message:"Screen name taken" );
  end if;
else
  MSG::status( status:status::error, message:"Already joined" );
end if;',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'',
	0);
