INSERT INTO S_SYNC
	VALUES ("c7270e69-54c1-4555-9a56-c0bcf7afc707",
	"00000000-0000-0000-0000-000000000000",
	'send',
	'',
	'select any user from instances of user where ( selected.endpoint == param.user );
if ( not_empty user )
  select many other_users from instances of user where ( selected.endpoint != param.user );
  for each other_user in other_users
    send MSG::message( name:user.name, message:param.message ) to other_user.endpoint;
  end for;
else
  MSG::status( status:status::error, message:"User not registered" );
end if;',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'',
	0);
